<%- include public/header %>


<div class="panel panel-primary">
  <div class="panel-heading"><span class="glyphicon glyphicon-send" aria-hidden="true"></span>&nbsp;最新专辑</div>
  <div class="panel-body">
      <div class="row">
        <% for(var i = 0;i < albums.length;i++){%>
          <div class="col-sm-6 col-md-2">
            <div class="thumbnail">
              <img class="img-thumbnail" src="<%=albums[i].pic%>" title="哎哟不错哦" alt="哎哟不错哦" />
              <div class="caption">
                <h3><a href="#"><%=albums[i].albumname%></a></h3>
                <p><a href="#"><%=albums[i].musican%></a></p>
                <p><a href="#" class="btn btn-primary" role="button">查看专辑</a></p>
              </div>
            </div>
          </div>
          <% } %>
      </div>
  </div>
</div>


<%- include public/footer %>